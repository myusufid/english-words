{
  "name": "English Words",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "English Words Everyday",
  "homepage_url": "https://myusuf.id",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/index.html"
  },
  "host_permissions": [
    "https://cdn.jsdelivr.net/*",
    "https://ajax.googleapis.com/*",
    "*://*/*"
  ]
}